    "graphql-scalars": "^1.23.0",
    "lodash": "^4.17.21",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "serve-favicon": "^2.5.0",
    "typeorm": "^0.3.20",
    "typeorm-transactional": "^0.5.0",
